<!-- SUBSCRIBE SECTION START-->
<section id="email-subscribe">
  <div class="container">
    <div class="row text-center">
      <div class="col-lg-6 col-lg-offset-3 col-md-6 col-md-offset-3 col-sm-6 col-sm-offset-3 col-xs-12">
        <p>Você pode pedir para <strong>lavar</strong> as roupas do seu <strong>pet</strong> pelo nosso formulário <strong>online.</strong></p>
        <p>Prencha as informações e vamos retirar as roupas em seu <strong>domicílio.</strong></p>

        <%= form_for(@order, class: 'form-horizontal') do |f| %>
            <div class="form-group">
              <%= f.label "Endereço"  %>
              <div>
                <%= f.text_field :address, class: 'form-control', placeholder: "Onde devemos buscar as roupinhas?" %>
              </div>
            </div>

            <div class="form-group">
              <%= f.label "Telefone" %>
              <div>
                <%= f.text_field :phone, class: 'form-control', placeholder: "Qual o telefone de confirmação?" %>
              </div>
            </div>

            <div class="form-group">
              <%= f.label "Nome" %>
              <div>
                <%= f.text_field :name, class: 'form-control', placeholder: "Qual o seu nome?"  %>
              </div>
            </div>

            <div class="form-group">
              <%= f.label "Descrição" %>
              <div>
                <%= f.text_field :description, class: 'form-control', placeholder: "Descreva seu pedido (opcional)"  %>
              </div>
            </div>

            <div class="actions">
              <%= f.submit 'Agendar pedido!', class: 'btn btn-subscribe'  %>
            </div>
        <% end %>
      </div>
    </div>
  </div>
</section>
<!-- SUBSCRIBE SECTION END-->
